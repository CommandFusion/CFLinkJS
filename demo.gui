        <active s="0">4</active>
      </btn>
      <btn j="0" x="44" y="150" w="106" h="43" t="top_btn_single" flip="None" sim="1" l="0" s="1" js="dev.mini.setRelayState([1,2,3,4], 0);">
        <inactive s="0">OPEN
ALL</inactive>
        <active s="0">OPEN
ALL</active>
      </btn>
      <btn j="0" x="169" y="150" w="106" h="43" t="top_btn_single" flip="None" sim="1" l="0" s="1" js="dev.mini.setRelayState([1,2,3,4], 1);">
        <inactive s="0">CLOSE ALL</inactive>
        <active s="0">CLOSE ALL</active>
      </btn>
      <txt j="0" x="0" y="340" w="320" h="34" t="text_c" wrap="False" l="0">RS232 Data:</txt>
      <txt j="5" x="0" y="366" w="320" h="34" t="small_c" wrap="False" l="0">nothing yet...</txt>
      <txt j="0" x="0" y="216" w="320" h="34" t="text_c" wrap="False" l="0">IO Ports</txt>
      <txt j="0" x="0" y="244" w="71" h="34" t="small_c" wrap="False" l="0">P01</txt>
      <txt j="0" x="83" y="244" w="71" h="34" t="small_c" wrap="False" l="0">P02</txt>
      <txt j="0" x="166" y="244" w="71" h="34" t="small_c" wrap="False" l="0">P03</txt>
      <txt j="0" x="249" y="244" w="71" h="34" t="small_c" wrap="False" l="0">P04</txt>
      <txt j="1" x="0" y="274" w="71" h="34" t="small_c" wrap="False" l="0">-</txt>
      <txt j="2" x="83" y="274" w="71" h="34" t="small_c" wrap="False" l="0">-</txt>
      <txt j="3" x="166" y="274" w="71" h="34" t="small_c" wrap="False" l="0">-</txt>
      <txt j="4" x="249" y="274" w="71" h="34" t="small_c" wrap="False" l="0">-</txt>
      <input j="0" s="6" x="12" y="406" w="300" h="23" pass="0" autoFocus="0" f="0" t="input" l="0">Send data to COM port...</input>
    </portrait>
    <landscape />
  </page>
  <subpage name="Cache" w="320" h="480" clip="1">
    <img x="19" y="22" w="40" h="40" j="0" clickthrough="0" refresh="0" l="0" noCache="0" downloadIndicator="0" clearOnURLChange="0">Padlock-Open.png</img>
    <img x="82" y="23" w="40" h="40" j="0" clickthrough="0" refresh="0" l="0" noCache="0" downloadIndicator="0" clearOnURLChange="0">Padlock.png</img>
  </subpage>
</gui>