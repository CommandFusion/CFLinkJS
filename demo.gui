<gui>
  <properties>
    <project>CFLinkJS Demo</project>
    <designer>CommandFusion</designer>
    <size>
      <portrait width="320" height="480" />
      <landscape width="480" height="320" />
    </size>
    <imagefolder>
    </imagefolder>
    <debug loaderrors="0" connectionerrors="0" />
  </properties>
  <systems>
    <system name="CFLINK" ip="192.168.0.115" port="10207" protocol="tcp" alwayson="1" offlinequeue="0" accept="0" acceptBroadcasts="0" ssl="0" maxconnections="0" heartbeatRx="" heartbeatTx="" heartbeatMode="-1" eom="\xF5\xF5" connectionStatus="0" disconnectionStatus="0">
      <fb name="IncomingData" regex="\xF2[\s\S]\xF3\w{7}\xF4[\s\S]*\xF5\xF5" />
      <fb name="ALL DATA" regex="." />
    </system>
    <system name="EpsonProjector" ip="192.168.0.115" port="10209" protocol="tcp" alwayson="1" offlinequeue="0" accept="0" acceptBroadcasts="0" ssl="0" maxconnections="0" heartbeatRx="" heartbeatTx="" heartbeatMode="-1" eom="\x0D" connectionStatus="0" disconnectionStatus="0">
      <fb name="EpsonProjectorFeedback" regex="." />
    </system>
  </systems>
  <themes>
    <theme type="background" name=".portrait"><![CDATA[ background-image: url(bg_portrait.png); background-repeat: no-repeat;]]></theme>
    <theme type="button" name=".top_btn_single[state='0']"><![CDATA[padding: 0px 21px 0px 21px; -webkit-border-image: url(top_btn_single_off.png) 0 21 0 21; color: White; font-size: 12px; font-family: 'Verdana'; font-weight: normal; font-style: none; text-decoration: none; text-align: center; vertical-align: middle; display: table-cell; box-sizing: border-box; -webkit-box-sizing: border-box; -webkit-tap-highlight-color:rgba(0,0,0,0);]]></theme>
    <theme type="button" name=".top_btn_single[state='1']"><![CDATA[padding: 0px 21px 0px 21px; -webkit-border-image: url(top_btn_single_on.png) 0 21 0 21; color: White; font-size: 12px; font-family: 'Verdana'; font-weight: normal; font-style: none; text-decoration: none; text-align: center; vertical-align: middle; display: table-cell; box-sizing: border-box; -webkit-box-sizing: border-box; -webkit-tap-highlight-color:rgba(0,0,0,0);]]></theme>
    <theme type="text" name=".text_c"><![CDATA[ text-shadow: rgba(0,0,0,1) 0px -1px 0px; color: Silver; font-size: 16px; font-family: 'Arial'; text-align: center; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="text" name=".status_c"><![CDATA[ text-shadow: rgba(0,0,0,1) 0px -1px 0px; color: Red; font-size: 12px; font-family: 'Arial'; text-align: center; vertical-align: middle; font-weight: normal; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="text" name=".small_c"><![CDATA[ text-shadow: rgba(0,0,0,1) 0px -1px 0px; color: White; font-size: 12px; font-family: 'Arial'; text-align: center; vertical-align: middle; font-weight: normal; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="input" name=".input"><![CDATA[ background-color: White; border-width: 1px; border-color: #575757; color: Black; font-size: 12px; font-family: 'Verdana'; text-align: left; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
  </themes>
  <scripts>
    <script name="cflink.js" />
    <script name="epsonprojector.js" />
    <script name="demo.js" />
  </scripts>
  <page name="Startup" j="0" transition="" subtype="" time="0" ease="">
    <portrait t="portrait">
      <txt j="0" x="0" y="3" w="320" h="41" t="text_c" wrap="False" l="0">CFLink JS Demo
- CF Mini -</txt>
      <btn j="1" x="22" y="90" w="66" h="43" t="top_btn_single" flip="None" sim="0" l="0" s="1" js="dev.mini.toggleRelayState(1);">
        <inactive s="0">1</inactive>
        <active s="0">1</active>
      </btn>
      <txt j="0" x="0" y="54" w="320" h="34" t="text_c" wrap="False" l="0">Relays</txt>
      <btn j="2" x="92" y="90" w="66" h="43" t="top_btn_single" flip="None" sim="0" l="0" s="1" js="dev.mini.toggleRelayState(2);">
        <inactive s="0">2</inactive>
        <active s="0">2</active>
      </btn>
      <btn j="3" x="162" y="90" w="66" h="43" t="top_btn_single" flip="None" sim="0" l="0" s="1" js="dev.mini.toggleRelayState(3);">
